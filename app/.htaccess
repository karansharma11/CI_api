<IfModule authz_core_module> Require all denied </IfModule>

<IfModule mod_headers.c>
  Header set Access-Control-Allow-Origin "*" Header set
  Access-Control-Allow-Methods "GET, POST, OPTIONS, PUT, DELETE" Header set
  Access-Control-Allow-Headers "Content-Type, Access-Control-Allow-Headers,
  Authorization, X-Requested-With"
</IfModule>

<IfModule !authz_core_module> Deny from all </IfModule>
